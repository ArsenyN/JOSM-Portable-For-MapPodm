/* relief */ 

way["relief"="contour"]
{
	color: #BF6100;
	width: 1.5;
}
way["relief"="hatch"]
{
	color: #BF6100;
	width: 1;
}
way["relief"="contour"]["deprecated:contour-width"="major"]
{
	color: #BF6100;
	width: 2.5;
}

way["relief"="cliff"]
{
	color: #BF6100;
	width: 2.5;	
	repeat-image: "cliff.png";
	repeat-image-spacing: 20;	
	repeat-image-align: top;
	repeat-image-height: 8;
}
way["relief"="ravine"]
{
	color: #BF6100;
	width: 3;		
}

